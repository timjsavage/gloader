<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
  <version>1004001</version>
  <userDir/>
  <connection>
    <proxyChain>
      <hostName/>
      <port><%= node['zap']['port'] %></port>
      <skipName/>
      <realm/>
      <userName/>
      <password/>
      <prompt>false</prompt>
    </proxyChain>
    <auth/>
    <timeoutInSecs>20</timeoutInSecs>
  </connection>
  <proxy>
    <ip>localhost</ip>
    <port><%= node['zap']['port'] %></port>
    <reverseProxy>
      <use>0</use>
      <ip>127.0.0.1</ip>
      <httpPort>80</httpPort>
      <httpsPort>443</httpsPort>
    </reverseProxy>
  </proxy>
  <view>
    <processImages>0</processImages>
    <displayOption>1</displayOption>
    <editorView>2</editorView>
    <brkPanelView>0</brkPanelView>
    <showMainToolbar>1</showMainToolbar>
    <locale>en_GB</locale>
    <advancedview>0</advancedview>
    <askOnExit>1</askOnExit>
    <uiWmHandling>0</uiWmHandling>
    <main>
      <request>
        <httppanel>
          <defaultcomponent>RequestSplit</defaultcomponent>
          <components>
            <dividerLocation>116</dividerLocation>
            <split>
              <header>
                <defaultview>text</defaultview>
                <views>
                  <text>
                    <aa>true</aa>
                    <linenumbers>false</linenumbers>
                    <wordwrap>true</wordwrap>
                    <highlightline>false</highlightline>
                    <fadehighlightline>false</fadehighlightline>
                    <whitespaces>false</whitespaces>
                    <newlines>false</newlines>
                    <markocurrences>false</markocurrences>
                    <roundedselection>false</roundedselection>
                    <bracketmatch>false</bracketmatch>
                    <animatedbracketmatch>false</animatedbracketmatch>
                  </text>
                </views>
              </header>
              <body>
                <defaultview>text</defaultview>
                <views>
                  <text>
                    <aa>true</aa>
                    <linenumbers>false</linenumbers>
                    <wordwrap>true</wordwrap>
                    <highlightline>false</highlightline>
                    <fadehighlightline>false</fadehighlightline>
                    <whitespaces>false</whitespaces>
                    <newlines>false</newlines>
                    <markocurrences>false</markocurrences>
                    <roundedselection>false</roundedselection>
                    <bracketmatch>false</bracketmatch>
                    <animatedbracketmatch>false</animatedbracketmatch>
                    <syntax>
                      <autodetect>true</autodetect>
                    </syntax>
                  </text>
                </views>
              </body>
            </split>
            <all>
              <defaultview>text</defaultview>
              <views>
                <text>
                  <aa>true</aa>
                  <linenumbers>false</linenumbers>
                  <wordwrap>true</wordwrap>
                  <highlightline>false</highlightline>
                  <fadehighlightline>false</fadehighlightline>
                  <whitespaces>false</whitespaces>
                  <newlines>false</newlines>
                  <markocurrences>false</markocurrences>
                  <roundedselection>false</roundedselection>
                  <bracketmatch>false</bracketmatch>
                  <animatedbracketmatch>false</animatedbracketmatch>
                </text>
              </views>
            </all>
          </components>
        </httppanel>
      </request>
      <response>
        <httppanel>
          <defaultcomponent>ResponseSplit</defaultcomponent>
          <components>
            <all>
              <defaultview>text</defaultview>
              <views>
                <text>
                  <aa>true</aa>
                  <linenumbers>false</linenumbers>
                  <wordwrap>true</wordwrap>
                  <highlightline>false</highlightline>
                  <fadehighlightline>false</fadehighlightline>
                  <whitespaces>false</whitespaces>
                  <newlines>false</newlines>
                  <markocurrences>false</markocurrences>
                  <roundedselection>false</roundedselection>
                  <bracketmatch>false</bracketmatch>
                  <animatedbracketmatch>false</animatedbracketmatch>
                </text>
              </views>
            </all>
            <dividerLocation>-1</dividerLocation>
            <split>
              <header>
                <defaultview>text</defaultview>
                <views>
                  <text>
                    <aa>true</aa>
                    <linenumbers>false</linenumbers>
                    <wordwrap>true</wordwrap>
                    <highlightline>false</highlightline>
                    <fadehighlightline>false</fadehighlightline>
                    <whitespaces>false</whitespaces>
                    <newlines>false</newlines>
                    <markocurrences>false</markocurrences>
                    <roundedselection>false</roundedselection>
                    <bracketmatch>false</bracketmatch>
                    <animatedbracketmatch>false</animatedbracketmatch>
                  </text>
                </views>
              </header>
              <body>
                <defaultview>text</defaultview>
                <views>
                  <text>
                    <aa>true</aa>
                    <linenumbers>false</linenumbers>
                    <wordwrap>true</wordwrap>
                    <highlightline>false</highlightline>
                    <fadehighlightline>false</fadehighlightline>
                    <whitespaces>false</whitespaces>
                    <newlines>false</newlines>
                    <markocurrences>false</markocurrences>
                    <roundedselection>false</roundedselection>
                    <bracketmatch>false</bracketmatch>
                    <animatedbracketmatch>false</animatedbracketmatch>
                    <syntax>
                      <autodetect>true</autodetect>
                    </syntax>
                  </text>
                </views>
              </body>
            </split>
          </components>
        </httppanel>
      </response>
    </main>
  </view>
  <start>
    <checkForUpdates>0</checkForUpdates>
    <dayLastChecked>Never</dayLastChecked>
  </start>
  <certificate>
    <use>0</use>
    <clientCertLocation/>
    <experimentalSlotListIndex>false</experimentalSlotListIndex>
  </certificate>
  <spider>
    <thread>2</thread>
    <maxDepth>5</maxDepth>
    <scope/>
    <postform>1</postform>
    <skipurl/>
  </spider>
  <trap>
    <inclusiveFilter/>
    <exclusiveFilter/>
  </trap>
  <scanner>
    <hostPerScan>2</hostPerScan>
    <threadPerHost>2</threadPerHost>
    <delayInMs>0</delayInMs>
    <antiCSFR>false</antiCSFR>
    <level>MEDIUM</level>
  </scanner>
  <portscan>
    <threadPerHost>5</threadPerHost>
    <maxPort>10240</maxPort>
    <timeoutInMs>100</timeoutInMs>
  </portscan>
  <bruteforce>
    <threadPerHost>10</threadPerHost>
    <recursive>true</recursive>
  </bruteforce>
  <pscans>
    <names>html_tag_form</names>
    <names>html_tag_password</names>
    <names>html_type_password</names>
    <names>html_type_hidden</names>
    <names>html_type_upload</names>
    <names>html_tag_object</names>
    <names>html_tag_script</names>
    <names>html_mailto</names>
    <names>html_setcookie</names>
    <names>html_comment1</names>
    <names>html_comment2</names>
    <html_tag_form>
      <type>TAG</type>
      <config>Form</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>&lt;form</resBodyRegex>
      <enabled>true</enabled>
    </html_tag_form>
    <html_tag_password>
      <type>TAG</type>
      <config>Password</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>&lt;password\s</resBodyRegex>
      <enabled>true</enabled>
    </html_tag_password>
    <html_type_password>
      <type>TAG</type>
      <config>Password</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>type\s*=\s*['"]?password['"]?</resBodyRegex>
      <enabled>true</enabled>
    </html_type_password>
    <html_type_hidden>
      <type>TAG</type>
      <config>Hidden</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>type\s*=\s*['"]?hidden['"]?</resBodyRegex>
      <enabled>true</enabled>
    </html_type_hidden>
    <html_type_upload>
      <type>TAG</type>
      <config>Upload</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>type\s*=\s*['"]?file['"]?</resBodyRegex>
      <enabled>true</enabled>
    </html_type_upload>
    <html_tag_object>
      <type>TAG</type>
      <config>Object</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>&lt;object\s</resBodyRegex>
      <enabled>true</enabled>
    </html_tag_object>
    <html_tag_script>
      <type>TAG</type>
      <config>Script</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>&lt;script</resBodyRegex>
      <enabled>true</enabled>
    </html_tag_script>
    <html_mailto>
      <type>TAG</type>
      <config>MailTo</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>&lt;.*href\s*['"]?mailto:</resBodyRegex>
      <enabled>true</enabled>
    </html_mailto>
    <html_setcookie>
      <type>TAG</type>
      <config>SetCookie</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex>set-cookie</resHeadRegex>
      <resBodyRegex/>
      <enabled>true</enabled>
    </html_setcookie>
    <html_comment1>
      <type>TAG</type>
      <config>Comment</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>/\*</resBodyRegex>
      <enabled>true</enabled>
    </html_comment1>
    <html_comment2>
      <type>TAG</type>
      <config>Comment</config>
      <reqUrlRegex/>
      <reqHeadRegex/>
      <resHeadRegex/>
      <resBodyRegex>&lt;!--</resBodyRegex>
      <enabled>true</enabled>
    </html_comment2>
  </pscans>
  <anticsrf>
    <tokens>CSRFToken</tokens>
    <tokens>__RequestVerificationToken</tokens>
    <tokens>anticsrf</tokens>
  </anticsrf>
  <plugins>
    <p10000>
      <enabled>1</enabled>
    </p10000>
    <p10001>
      <enabled>1</enabled>
    </p10001>
    <p40000>
      <enabled>1</enabled>
    </p40000>
    <p40001>
      <enabled>1</enabled>
    </p40001>
    <p40002>
      <enabled>1</enabled>
    </p40002>
    <p20000>
      <enabled>1</enabled>
    </p20000>
    <p20001>
      <enabled>1</enabled>
    </p20001>
