      <enabled>1</enabled>
    </p0>
    <p30000>
      <enabled>1</enabled>
    </p30000>
    <p1>
      <enabled>1</enabled>
    </p1>
    <p2>
      <enabled>1</enabled>
    </p2>
    <p3>
      <enabled>1</enabled>
    </p3>
    <p40003>
      <enabled>1</enabled>
    </p40003>
    <p40006>
      <enabled>1</enabled>
    </p40006>
    <p40007>
      <enabled>1</enabled>
    </p40007>
    <p40005>
      <enabled>1</enabled>
    </p40005>
    <p40004>
      <enabled>1</enabled>
    </p40004>
    <p4>
      <enabled>1</enabled>
    </p4>
    <p5>
      <enabled>1</enabled>
    </p5>
    <p40008>
      <enabled>1</enabled>
    </p40008>
    <p40009>
      <enabled>1</enabled>
    </p40009>
    <p40010>
      <enabled>1</enabled>
    </p40010>
    <p40011>
      <enabled>1</enabled>
    </p40011>
    <p40012>
      <enabled>1</enabled>
    </p40012>
    <p6>
      <enabled>1</enabled>
    </p6>
    <p20010>
      <enabled>1</enabled>
    </p20010>
  </plugins>
  <fuzzer>
    <threadPerScan>1</threadPerScan>
    <defaultCategory>fuzzdb-1.09 / Discovery / FilenameBruteforce</defaultCategory>
    <delayInMs>0</delayInMs>
  </fuzzer>
  <ext>
    <ExtensionAPI>true</ExtensionAPI>
    <ExtensionActiveScan>true</ExtensionActiveScan>
    <ExtensionAlert>true</ExtensionAlert>
    <ExtensionAntiCSRF>true</ExtensionAntiCSRF>
    <ExtensionAutoUpdate>true</ExtensionAutoUpdate>
    <ExtensionBeanShell>true</ExtensionBeanShell>
    <ExtensionBreak>true</ExtensionBreak>
    <ExtensionBruteForce>true</ExtensionBruteForce>
    <ExtensionCompare>true</ExtensionCompare>
    <ExtensionCopy>true</ExtensionCopy>
    <ExtensionDynSSL>true</ExtensionDynSSL>
    <ExtensionEdit>true</ExtensionEdit>
    <ExtensionEncode2>true</ExtensionEncode2>
    <ExtensionExtension>true</ExtensionExtension>
    <ExtensionFilter>true</ExtensionFilter>
    <ExtensionFuzz>true</ExtensionFuzz>
    <ExtensionHelp>true</ExtensionHelp>
    <ExtensionHistory>true</ExtensionHistory>
    <ExtensionHttpPanelComponentAll>true</ExtensionHttpPanelComponentAll>
    <ExtensionHttpPanelHexView>true</ExtensionHttpPanelHexView>
    <ExtensionHttpPanelImageView>true</ExtensionHttpPanelImageView>
    <ExtensionHttpPanelLargeResponseView>true</ExtensionHttpPanelLargeResponseView>
    <ExtensionHttpPanelRequestFormTableView>true</ExtensionHttpPanelRequestFormTableView>
    <ExtensionHttpPanelRequestQueryCookieTableView>true</ExtensionHttpPanelRequestQueryCookieTableView>
    <ExtensionHttpPanelSyntaxHighlightTextView>true</ExtensionHttpPanelSyntaxHighlightTextView>
    <ExtensionInvoke>true</ExtensionInvoke>
    <ExtensionLog4j>true</ExtensionLog4j>
    <ExtensionManualRequest>true</ExtensionManualRequest>
