    <ExtensionReveal>true</ExtensionReveal>
    <ExtensionSaveRawHttpMessage>true</ExtensionSaveRawHttpMessage>
    <ExtensionSearch2>true</ExtensionSearch2>
    <ExtensionSession>true</ExtensionSession>
    <ExtensionSitesRefresh>true</ExtensionSitesRefresh>
    <ExtensionSpider2>true</ExtensionSpider2>
    <ExtensionStandardMenus>true</ExtensionStandardMenus>
    <ExtensionState>true</ExtensionState>
    <ExtensionViewOption>true</ExtensionViewOption>
