  <session>
    <tokens>asp.net_sessionid</tokens>
    <tokens>aspsessionid</tokens>
    <tokens>cfid</tokens>
    <tokens>cftoken</tokens>
    <tokens>jsessionid</tokens>
    <tokens>phpsessid</tokens>
    <tokens>sessid</tokens>
    <tokens>sid</tokens>
    <tokens>siteserver</tokens>
